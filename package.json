{
  "name": "iqueue",
  "private": true,
  "version": "3.0.0",
  "description": "An SMS paging API and web client",
  "engines": {
    "node": ">=4.2.4",
    "npm": ">=3.5.2"
  },
  "keywords": [
    "sms",
    "paging",
    "waitlist"
  ],
  "dependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.4.3",
    "boom": "^3.1.1",
    "dateformat": "^1.0.11",
    "dogwater": "^1.0.1",
    "flux": "^2.1.1",
    "good": "^6.3.0",
    "good-console": "^5.0.2",
    "hapi": "^12.1.0",
    "hapi-react-views": "^5.0.2",
    "hapi-swagger": "^3.1.1",
    "immutable": "^3.7.6",
    "inert": "^3.0.1",
    "joi": "^7.2.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.3",
    "require-directory": "^2.1.1",
    "sails-disk": "^0.10.8",
    "superagent": "^1.2.0",
    "twilio": "^2.2.1",
    "vision": "^4.0.1",
    "waterline": "^0.10.26"
  },
  "scripts": {
    "build": "webpack -p --progress --config webpack/config.js",
    "debug": "node debug api/server.js",
    "dev": "NODE_ENV=development npm run watch-assets -- & npm run watch-api",
    "lint": "eslint --ignore-pattern **/bundle-*.js --ext .js,.jsx api/** src/**",
    "start": "node api/server.js",
    "test": "jest --verbose --coverage",
    "test-lab": "lab -v -P '_test' -T node_modules/lab-babel -t 97 api src",
    "watch": "nodemon -w api/ -w src/ -e js,jsx",
    "watch-api": "nodemon -w api/ -e js,jsx",
    "watch-assets": "webpack --progress --colors --watch --watch-aggregate-timeout 1000 --config webpack/config.js",
    "watch-test": "npm run watch -- -x npm run test"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  },
  "main": "api/server.js",
  "repository": {
    "type": "git",
    "url": "git://github.com:triconn/iqueue.git"
  },
  "author": "triconn",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "browser-sync": "^2.9.3",
    "browser-sync-webpack-plugin": "^1.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^1.6.0",
    "eslint-config-hapi": "^8.0.1",
    "eslint-plugin-hapi": "^4.0.0",
    "eslint-plugin-react": "^3.5.1",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "nodemon": "^1.3.7",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  }
}
