{
  "name": "iqueue",
  "version": "4.0.0",
  "description": "An SMS paging API and web client",
  "engines": {
    "node": ">=4.2.4",
    "npm": ">=3.5.2"
  },
  "keywords": [
    "sms",
    "paging",
    "waitlist"
  ],
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.4.3",
    "boom": "^3.1.1",
    "dateformat": "^1.0.11",
    "debug": "^2.2.0",
    "flux": "^2.1.1",
    "good": "^6.3.0",
    "good-console": "^5.0.2",
    "hapi": "^13.0.0",
    "hapi-react-views": "^7.0.0",
    "hapi-webpack-plugin": "^1.3.0",
    "immutable": "^3.7.6",
    "inert": "^4.0.0",
    "joi": "^8.0.1",
    "key-mirror": "^1.0.1",
    "material-ui": "^0.15.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.0.5",
    "redux-thunk": "^2.1.0",
    "require-directory": "^2.1.1",
    "superagent": "^1.2.0",
    "vision": "^4.0.1"
  },
  "scripts": {
    "build": "webpack -p --progress --config src/config/webpack.production.js",
    "build-dev": "webpack --progress --colors --config src/config/webpack.js",
    "build-staging": "docker-compose -f docker/production.yml build",
    "debug": "node debug src/index.bootstrap.server.js",
    "deploy-dev": "docker-compose -f docker/development.yml build; docker-compose -f docker/development.yml up",
    "deploy-staging": "npm run build-staging; docker-compose -f docker/production.yml up -d",
    "dev": "NODE_ENV=development npm run start-dev",
    "flow": "flow",
    "lint": "eslint --ignore-pattern **/bundle-*.js --ext .js,.jsx src/**",
    "start": "node src/index.bootstrap.server.js",
    "start-dev": "node src/index.bootstrap.server.js",
    "test": "lab -v -P '_test' -T node_modules/lab-babel -I __core-js_shared__ -t 97 src",
    "watch": "nodemon -w src/config/ -w src/controllers/ -w src/helpers/ -w src/models/ -w src/index.bootstrap.server.js -e js -x npm run dev",
    "watch-test": "nodemon -w src/ -e js,jsx -x npm run test"
  },
  "main": "src/index.bootstrap.server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/triconn/iqueue.git"
  },
  "author": "triconn",
  "license": "Apache-2.0",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.4.0",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "browser-sync": "^2.9.3",
    "browser-sync-webpack-plugin": "^1.0.0",
    "code": "^2.1.0",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-tribou": "^1.1.0",
    "eslint-plugin-flow-vars": "^0.4.0",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-standard": "^1.3.2",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^0.8.5",
    "flow-bin": "^0.24.2",
    "hapi-swagger": "^4.1.0",
    "json-loader": "^0.5.4",
    "lab": "^8.2.0",
    "lab-babel": "^1.1.1",
    "node-sass": "^3.4.2",
    "nodemon": "^1.3.7",
    "npm-install-webpack-plugin": "^2.0.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11"
  }
}
