{
  "name": "iqueue",
  "private": true,
  "version": "2.1.1",
  "description": "An SMS paging API and web client",
  "engines": {
    "node": "0.10.x"
  },
  "keywords": [
    "sms",
    "paging",
    "waitlist"
  ],
  "dependencies": {
    "boom": "^2.8.0",
    "dateformat": "^1.0.11",
    "dogwater": "^0.4.6",
    "flux": "^2.0.3",
    "good": "^6.3.0",
    "good-console": "^5.0.2",
    "hapi": "^9.3.1",
    "hapi-react-views": "^3.0.1",
    "hapi-swagger": "^2.0.0",
    "inert": "^3.0.1",
    "joi": "^6.6.1",
    "object-assign": "^3.0.0",
    "react": "^0.13.3",
    "require-directory": "^2.1.1",
    "sails-disk": "^0.10.8",
    "sails-mongo": "^0.11.2",
    "superagent": "^1.2.0",
    "twilio": "^2.2.1",
    "vision": "^3.0.0",
    "waterline": "^0.10.26"
  },
  "scripts": {
    "start": "node api/server.js",
    "build": "webpack -p --progress --config build/webpack.production.config.js",
    "debug": "node debug api/server.js",
    "dev": "webpack --progress --colors --watch --config build/webpack.config.js & nodemon",
    "lint": "eslint --ignore-pattern **/bundle-*.js --ext .js,.jsx web/**",
    "test": "jest --verbose --coverage"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": ["es6", "js"],
    "moduleFileExtensions": ["js", "json", "es6"]
  },
  "main": "api/server.js",
  "repository": {
    "type": "git",
    "url": "git://github.com:triconn/iqueue.git"
  },
  "author": "triconn",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "browser-sync": "^2.9.3",
    "browser-sync-webpack-plugin": "^1.0.0",
    "css-loader": "^0.16.0",
    "eslint": "^1.6.0",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "^3.5.1",
    "jest-cli": "^0.5.10",
    "nodemon": "^1.3.7",
    "style-loader": "^0.12.3",
    "webpack": "^1.10.3"
  }
}
