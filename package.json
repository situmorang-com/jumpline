{
  "name": "iqueue",
  "private": true,
  "version": "3.0.1",
  "description": "An SMS paging API and web client",
  "engines": {
    "node": ">=4.2.4",
    "npm": ">=3.5.2"
  },
  "keywords": [
    "sms",
    "paging",
    "waitlist"
  ],
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.4.3",
    "boom": "^3.1.1",
    "dateformat": "^1.0.11",
    "debug": "^2.2.0",
    "dogwater": "^1.0.1",
    "flux": "^2.1.1",
    "good": "^6.3.0",
    "good-console": "^5.0.2",
    "hapi": "^13.0.0",
    "hapi-react-views": "^5.0.2",
    "hapi-webpack-plugin": "^1.3.0",
    "immutable": "^3.7.6",
    "inert": "^3.0.1",
    "joi": "^7.2.1",
    "key-mirror": "^1.0.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.3",
    "require-directory": "^2.1.1",
    "sails-disk": "^0.10.8",
    "superagent": "^1.2.0",
    "twilio": "^2.2.1",
    "vision": "^4.0.1",
    "waterline": "^0.10.26"
  },
  "scripts": {
    "build": "webpack -p --progress --config src/config/webpack.production.js",
    "build-dev": "webpack --progress --colors --config src/config/webpack.js",
    "debug": "node debug src/index.server.js",
    "dev": "NODE_ENV=development npm start",
    "lint": "eslint --ignore-pattern **/bundle-*.js --ext .js,.jsx src/**",
    "start": "node src/index.server.js",
    "test": "lab -v -P '_test' -T node_modules/lab-babel -t 97 src",
    "watch": "nodemon -w src/config/ -w src/controllers/ -w src/helpers/ -w src/models/ -w src/index.server.js -e js -x npm run dev",
    "watch-test": "nodemon -w src/ -e js,jsx -x npm run test"
  },
  "main": "src/index.server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/triconn/iqueue.git"
  },
  "author": "triconn",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-react-hmre": "^1.1.0",
    "browser-sync": "^2.9.3",
    "browser-sync-webpack-plugin": "^1.0.0",
    "code": "^2.1.0",
    "css-loader": "^0.23.1",
    "eslint": "^1.6.0",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-config-hapi": "^8.0.1",
    "eslint-plugin-hapi": "^4.0.0",
    "eslint-plugin-react": "^3.5.1",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^0.8.5",
    "hapi-swagger": "^4.1.0",
    "lab": "^8.2.0",
    "lab-babel": "^1.1.1",
    "node-sass": "^3.4.2",
    "nodemon": "^1.3.7",
    "npm-install-webpack-plugin": "^2.0.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11"
  }
}
